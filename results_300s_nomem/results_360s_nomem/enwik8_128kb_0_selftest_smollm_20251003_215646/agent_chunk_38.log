n            actual_text_start_index = text_content_start_relative_index + len(text_content_start_marker)\n            \n            # Find the end of the Autism page's text content\n            text_content_end_marker = \"</text>\"\n            text_content_end_relative_index = raw_text_after_prefix.find(text_content_end_marker, actual_text_start_index)\n            \n            if text_content_end_relative_index != -1:\n                raw_continuation_text = raw_text_after_prefix[actual_text_start_index:text_content_end_relative_index]\n            else:\n                raw_continuation_text = raw_text_after_prefix[actual_text_start_index:]\n            \n            # Apply cleaning rules\n            cleaned_continuation = re.sub(r'\\[\\[[^|\\]]*\\|([^\\]]+)\\]\\]', r'\\1', raw_continuation_text) # [[Link|Text]] -> Text\n            cleaned_continuation = re.sub(r'\\[\\[([^\\]]+)\\]\\]', r'\\1', cleaned_continuation) # [[Link]] -> Link\n            cleaned_continuation = re.sub(r'\\[http[^\\]]* ([^\\]]+)\\]', r'\\1', cleaned_continuation) # [http://... Text] -> Text\n            cleaned_continuation = re.sub(r'\\[http[^\\]]*\\]', '', cleaned_continuation) # [http://...] -> (empty)\n            cleaned_continuation = cleaned_continuation.replace(\"''\", \"\") # Remove italics markers\n            cleaned_continuation = cleaned_continuation.replace(\"'''\", \"\") # Remove bold markers\n            cleaned_continuation = cleaned_continuation.replace(\"&quot;\", \"\\\"\") # Replace HTML entity\n            cleaned_continuation = cleaned_continuation.replace(\"&amp;mdash;\", \"\u2014\") # Replace HTML entity\n            cleaned_continuation = cleaned_continuation.replace(\"&amp;\", \"&\") # Replace HTML entity\n            cleaned_continuation = cleaned_continuation.replace(\"{{fact}}\", \"\") # Remove fact tags\n            \n            # Remove other Wikipedia templates and comments that might be present\n            cleaned_continuation = re.sub(r'\\{\\{[^}]*